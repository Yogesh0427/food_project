<html>
    <head>
        <script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
        <script src="/javascripts/poster.js"></script> 
        <link href="/stylesheets/displayall_data.css" rel="stylesheet"/>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Barlow+Condensed:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Dosis:wght@200..800&family=Noto+Serif:ital,wght@0,100..900;1,100..900&display=swap" rel="stylesheet">
    </head>
    <body>
        <div class="screen">
            <div style="height: 100%; width: 100%; display: flex; justify-content: center; align-items: center; background-color: rgba(0, 0, 0, 0.55);">
            <div class="form">
                <div class="shadow p-3 mb-5 rounded box" style="background: #ffffffbb;">
                    <div class="container">  
                        <table class="table table-striped table-hover"> 
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="titeldiv1">
                                        <div>
                                            <img src="/images/logo2.png" class="logo">
                                        </div>
                                       <div class="heading">Fame Of Food</div>
                                    </div>
                                    <div class="titeldiv2">
                                        <div class="line"></div>
                                            <div class="secoundtitel">List of dishis</div>   
                                    </div>
                                </div>
                            </div>
                            <thead>
                                <tr>
                                    <th class="col">#</th>
                                    <th class="col">Category</th>
                                    <th class="col">Subcategory</th>
                                    <th class="col">Dish</th>
                                    <th class="col">Ingredients</th>
                                    <th class="col">Price</th>
                                    <th class="col">Offer-Price</th>
                                    <th class="col">Status</th>
                                    <th class="col">Food Type</th>
                                    <th class="col">Taste</th>
                                    <th class="col">Picture</th>
                                    <th class="col">Edit</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% data.map((item,i)=>{ %>
                                    <tr>
                                        <td><%=item.fooddata_id %></td>
                                        <td><%= item.category_name %></td>
                                        <td><%= item.subcategory_name %></td>
                                        <td><%= item.food_name %></td>
                                        <td><%= item.ingridients %></td>
                                        <td><%= item.price %></td>
                                        <td><%= item.offer_price %></td>
                                        <td><%= item.status %></td>
                                        <td><%= item.food_type %></td>
                                        <td><%= item.taste %></td>
                                        <td><a href="/food/edit_image?foodid=<%=item.fooddata_id %>&foodname=<%=item.food_name %>&poster=<%=item.picture %>"><img class="picture" src=/images/<%= item.picture %>></a></td>
                                        <td><a href="/food/edit_details?foodid=<%=item.fooddata_id %>">Edit</a></td>
                                    </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            </div> </div>
    </body>
</html>