<html>
    <head>
        <script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
        <script src="/javascripts/poster.js"></script> 
        <link href="/stylesheets/food_interface.css" rel="stylesheet"/>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Barlow+Condensed:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Dosis:wght@200..800&family=Noto+Serif:ital,wght@0,100..900;1,100..900&display=swap" rel="stylesheet">
    </head>
    <body>
        <form action="/food/edit_details"> 
            <div class="screen">
                <div style="height: 100%; width: 100%; display: flex; justify-content: center; align-items: center; background-color: rgba(0, 0, 0, 0.5);">
                <div class="form">
                    <div class="shadow p-3 mb-5 rounded box"  style="background: #ffffff56; ">
                        <div class="container">    
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="titeldiv1">
                                        <div>
                                            <img src="/images/logo2.png" class="logo">
                                        </div>
                                        <div class="heading">Fame Of Food</div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="titeldiv2">
                                        <div class="line"></div>
                                        <div class="heading2">Find Foods</div>
                                    </div>
                                </div>
                            </div>
                            <div style="width: 300px; margin-top: 20px; display: flex; justify-content: center; align-items: center;">
                                <input type="text" name="foodid" placeholder="Enter Food Id" class="form-control">
                            </div>
                            <div class="row" style="margin-top: 20px;">
                                <div class="col-sm-6" style="display: flex; justify-content: center;">
                                    <button type="reset" class="btn btn-outline-danger">Reset</button>
                                </div>
                                <div class="col-sm-6" style="display: flex; justify-content: center;">
                                    <button type="submit" class="btn btn-outline-primary">Check</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div></div>
        </form>
    </body>
